<!DOCTYPE html>
<html>

	<head>
		<meta charset="utf-8" />
		<title></title>
		<style type="text/css">#container {
	perspective: 3500px;
	width: 1100px;
	height: 800px;
	margin: 140px auto;
}

#stage {
	height: 300px;
	width: 900px;
	transform-style: preserve-3d;
	transform: rotateY(0) rotateX(-5deg);
	position: relative;
	margin: 0 auto;
}

img {
	position: absolute;
	left: 40%;
	top: 23%;
	width: 210px;
	height: 180px;
	/*backface-visibility: hidden;*/
}</style>
		<script src="jquery-1.11.3.js" type="text/javascript" charset="utf-8"></script>
		
	</head>

	<body>
		<div id="container">
			<div id="stage">
				<img src="img/agd.jpg" style="-moz-transform: rotateY(0deg) translateZ(460px);" />
				<img src="img/agf.jpg" style="-moz-transform: rotateY(30deg) translateZ(460px);" />
				<img src="img/g.jpg" style="-moz-transform: rotateY(60deg) translateZ(460px);" />
				<img src="img/gs.jpg" style="-moz-transform: rotateY(90deg) translateZ(460px);" />
				<img src="img/gsa.jpg" style="-moz-transform: rotateY(120deg) translateZ(460px);" />
				<img src="img/k.jpg" style="-moz-transform: rotateY(150deg) translateZ(460px);" />
				<img src="img/p.jpg" style="-moz-transform: rotateY(180deg) translateZ(460px);" />
				<img src="img/rdsre.jpg" style="-moz-transform: rotateY(210deg) translateZ(460px);" />
				<img src="img/rsag.jpg" style="-moz-transform: rotateY(240deg) translateZ(460px);" />
				<img src="img/sdfg.jpg" style="-moz-transform: rotateY(270deg) translateZ(460px);" />
				<img src="img/sdhgfr.jpg" style="-moz-transform: rotateY(300deg) translateZ(460px);" />
				<img src="img/shgs.jpg" style="-moz-transform: rotateY(330deg) translateZ(460px);" />
			</div>
		</div>
	</body>
	<script type="text/javascript">$("#stage").mouseenter(function(e) {
	var x0 = e.clientX;
	var Y0 = e.clientY;
	$(document).mousemove(function(e) {
			var x1 = e.clientX;
			var Y1 = e.clientY;
			var x = x1 - x0;
			var Y = Y1 - Y0;

			x = x / 2;
			Y = Y / 2;
			$("#stage").css({
				"transform": "rotateY(" + x + "deg) rotateX(" + Y + "deg)",
				"transition": "0s",
				"transform-origin": "50% 50%"
			});

		})
		// $(document).mouseup(function(){
		// 	$(document).unbind("mousemove");
		// 	})
})</script>

</html>